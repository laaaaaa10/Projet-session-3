// ************** INCLUDES ************** // 
#include "main.h"
#include "Keyboard.h"
#include <stdbool.h>
#include <stusdio.h>

// ************** VARIABLES ************ //

// ************** FUNCTIONS ************ //

// addition function
void add(Operandes *operandes) {
    operandes->resultat = operandes->a + operandes->b;
}

// subtraction function
void sub(Operandes *operandes) {
    operandes->resultat = operandes->a - operandes->b;
}

// multiplication function
void mul(Operandes *operandes) {
    operandes->resultat = operandes->a * operandes->b;
}